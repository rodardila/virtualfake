{
    "executionRoleArn": "arn:aws:iam::327840057187:role/ecsTaskExecutionRole",
    "family": "virtualfaker-task",
    "containerDefinitions": [
        {
            "name": "virtualfaker",
            "image": "<IMAGE_URI>",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512"
}